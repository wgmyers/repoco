    "nyc": "^15.1.0",
    "sass": "^1.45.0",
    "should": "^13.2.3",
    "supertest": "^6.1.6"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "bootstrap": "^5.1.3",
    "connect-flash": "^0.1.1",
    "dotenv": "^10.0.0",
    "easymde": "^2.15.0",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "jquery": "^3.6.0",
    "jquery.fancytree": "^2.38.0",
    "js-yaml": "^4.1.0",
    "memorystore": "^1.6.6",
    "mongoose": "^6.1.2",
    "morgan": "^1.10.0",
    "passport": "^0.5.2",
