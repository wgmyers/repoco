{
  "name": "repoco",
  "version": "0.5.19",
  "description": "A minimalist headless CMS with local git repo support",
  "repository": "github:wgmyers/repoco",
  "author": {
    "name": "Wayne Myers",
    "email": "wgm@yutani.org",
    "url": "https://www.yutani.org"
  },
  "main": "bin/www",
  "scripts": {
    "prod": "NODE_ENV=production node ./bin/www",
    "dev": "nodemon ./bin/www",
    "build": "sass scss:public/css",
    "scss": "sass --watch scss:public/css",
    "lint": "eslint *.js lib/*.js public/js/*.js",
    "test": "mocha --exit",
    "coverage": "nyc npm run test"
  },
  "keywords": [
    "CMS",
    "headless",
    "jekyll",
    "git",
    "local",
    "self-host"
  ],
  "license": "GPL-3.0-only",
  "devDependencies": {
    "chai": "^4.3.6",
    "eslint": "^8.23.1",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "sass": "^1.55.0",
    "should": "^13.2.3",
    "supertest": "^6.2.4"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "bootstrap": "^5.1.3",
    "connect-flash": "^0.1.1",
    "dotenv": "^16.0.1",
    "easymde": "^2.18.0",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "jquery": "^3.6.0",
    "jquery.fancytree": "^2.38.2",
    "js-yaml": "^4.1.0",
    "memorystore": "^1.6.7",
    "mongoose": "^6.6.1",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^7.1.2",
    "pug": "^3.0.2",
    "serve-favicon": "^2.5.0",
    "simple-git": "^3.14.1"
  }
}
